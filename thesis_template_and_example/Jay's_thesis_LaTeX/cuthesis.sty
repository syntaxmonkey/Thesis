                      \@makechapterhead{#2}%
                      \@afterheading
                    \fi}


\def\newappendix#1{%
  \ifnum0=\value{myappendixcount}
    \startappendix
    \setcounter{myappendixcount}{1}
  \fi
  \include{#1}
}

